{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$id": "urn:nuage-metroag:credentials",
    "title": "Credentials",
    "description": "Nuage MetroAG credentials",
    "type": "object",
    "widget": "form",
    "properties": {
        "vsd_auth_username": {
            "type": "string",
            "title": "VSD API/Architect username",
            "default": "csproot",
            "propertyOrder": 10
        },
        "vsd_auth_password": {
            "type": "string",
            "title": "VSD API/Architect password",
            "default": "csproot",
            "propertyOrder": 20
        },
        "vsd_auth_enterprise": {
            "type": "string",
            "title": "VSD API/Architect Enterprise",
            "default": "csp",
            "propertyOrder": 30
        },
        "vsd_username": {
            "type": "string",
            "title": "VSD system username",
            "default": "root",
            "propertyOrder": 40
        },
        "vsd_password": {
            "type": "string",
            "title": "VSD system password",
            "default": "Alcateldc",
            "propertyOrder": 50
        },
        "vsc_username": {
            "type": "string",
            "title": "VSC system username",
            "default": "admin",
            "propertyOrder": 60
        },
        "vsc_password": {
            "type": "string",
            "title": "VSC system password",
            "default": "admin",
            "propertyOrder": 70
        },
        "vstat_username": {
            "type": "string",
            "title": "VSD Stats (VSTAT) system username",
            "default": "root",
            "propertyOrder": 80
        },
        "vstat_password": {
            "type": "string",
            "description": "VSD Stats (VSTAT) system password",
            "default": "Alcateldc",
            "propertyOrder": 90
        }
    }
}
