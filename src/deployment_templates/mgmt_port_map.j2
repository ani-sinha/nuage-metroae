###############################################################################
# Management Port Map
#
# DESCRIPTION TO BE WRITTEN
#
# Automatically generated by {{ generator_script | default("script") }}.
#

{% if port_maps is defined and port_maps %}
{% for item in port_maps %}
#
# Port Map {{ loop.index }}
#
-
    ##### Management Switch

    # < Slot >
    # DESCRIPTION TO BE WRITTEN
    #
    {%- if item.slot is defined %}
    slot: {{ item.slot }}
    {%- else %}
    # slot: 0
    {%- endif %}

    # < Port >
    # DESCRIPTION TO BE WRITTEN
    #
    {%- if item.port is defined %}
    port: {{ item.port }}
    {%- else %}
    # port: 0
    {%- endif %}

    #######################

    ##### Airframe Server

    # < Rack Position >
    # DESCRIPTION TO BE WRITTEN
    #
    rack_position: "{{ item.rack_position }}"

    # < Type >
    # DESCRIPTION TO BE WRITTEN
    # (Blank, Spine, Leaf, Cable Management, Nuage Node, Node, Storage Node, Mgmt Switch, Inception Server, CBIS UC) 
    #
    type: "{{ item.type }}"

    # < Rack Item Name >
    # DESCRIPTION TO BE WRITTEN
    #
    {%- if item.rack_item_name is defined %}
    rack_item_name: "{{ item.rack_item_name }}"
    {%- else %}
    # rack_item_name: ""
    {%- endif %}

    # < CBIS Visibility >
    # DESCRIPTION TO BE WRITTEN
    # (Visibile, Not Visibile) 
    #
    {%- if item.cbis_visibility is defined %}
    cbis_visibility: "{{ item.cbis_visibility }}"
    {%- else %}
    # cbis_visibility: ""
    {%- endif %}

    # < Inband IP Address/Netmask >
    # DESCRIPTION TO BE WRITTEN
    #
    {%- if item.inband_ip_address_mask is defined %}
    inband_ip_address_mask: "{{ item.inband_ip_address_mask }}"
    {%- else %}
    # inband_ip_address_mask: ""
    {%- endif %}

    #####################

    ##### Top Slot

    # < OOB IP Address/Netmask >
    # DESCRIPTION TO BE WRITTEN
    #
    {%- if item.oob_ip_address_mask is defined %}
    oob_ip_address_mask: "{{ item.oob_ip_address_mask }}"
    {%- else %}
    # oob_ip_address_mask: ""
    {%- endif %}

    # < P3(ens31f0) >
    # DESCRIPTION TO BE WRITTEN
    #
    {%- if item.p3 is defined %}
    p3: "{{ item.p3 }}"
    {%- else %}
    # p3: ""
    {%- endif %}

    # < P4(ens31f1) >
    # DESCRIPTION TO BE WRITTEN
    #
    {%- if item.p4 is defined %}
