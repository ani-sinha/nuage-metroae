# *** WARNING ***
# This is a generated file. Manual changes to this file
# will be lost if reset-build or build is run
#
target_server_type: {{ item.target_server_type }}
{% if item.target_server_type == "kvm" or item.target_server_type == "vcenter"%}
target_server: {{ item.target_server }}

interfaces:
  mgmt:
    ip: {{ item.mgmt_ip }}
    gateway: {{ item.mgmt_gateway }}
    netmask: {{ item.mgmt_netmask }}
{% if item.target_server_type == "kvm" %}
vsd_qcow2_path: {{ vsd_qcow2_path }}
vsd_qcow2_file_name: {{ vsd_qcow2_file_name }}
{% endif %}
{% if item.target_server_type == "vcenter" %}
vsd_ova_path: {{ vsd_ova_path }}
vsd_ova_file_name: {{ vsd_ova_file_name }}
{% endif %}
{% endif %}

{% if vsd_standalone is defined %}
vsd_standalone: {{ vsd_standalone }}
{% else %}
vsd_standalone: True
{% endif %}

{% if item.target_server_type == "heat" %}
vsd_image: {{ item.vsd_image }}
vsd_flavor: {{ item.vsd_flavor }}
vsd_network: {{ item.vsd_network }}
infra_server_name: {{ item.infra_server_name }}
{% endif %}
