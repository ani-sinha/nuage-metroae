target_server_type: {{ item.target_server_type }}
{% if item.target_server_type == "kvm" %}
target_server: {{ item.target_server }}

interfaces:
  mgmt:
    ip: {{ item.mgmt_ip }}
    netmask_prefix: {{ item.mgmt_netmask_prefix }}
    gateway: {{ item.mgmt_gateway }}
  control:
    ip: {{ item.ctrl_ip }}
    netmask_prefix: {{ item.ctrl_netmask_prefix }}

vsc_static_route_list:
{% for route in item.vsc_static_route_list %}
  - {{ route }}
{% endfor %}

vsc_qcow2_path: {{ vsc_qcow2_path }}

vsc_qcow2_file_name: {{ vsc_qcow2_file_name }}
{% endif %}

vsd_fqdn: {{ item.vsd_fqdn }}
system_ip: {{ item.system_ip }}
xmpp:
  username: {{ item.xmpp_username }}

{% if item.target_server_type == "heat" %}
vsc_image: {{ item.vsc_image }}
vsc_flavor: {{ item.vsc_flavor }}
vsc_mgmt_network: {{ item.vsc_mgmt_network }}
vsc_control_network: {{ item.vsc_control_network }}
system_ip: {{ item.system_ip }}
xmpp_username: {{ item.xmpp_username }}
infra_server_name: {{ item.infra_server_name }}
{% endif %}
